{
  "name": "ReactRelayOfflineNativeExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cli": "node node_modules/react-native/local-cli/cli.js",
    "start": "npm run cli -- start",
    "start-reset": "react-native start --reset-cache",
    "run-android": "npm run cli -- run-android",
    "debug": "react-native log-android",
    "relay:compile": "relay-compiler --src ./src --schema ./server/data/schema.graphql --language typescript",
    "test": "jest",
    "netinfo": "react-native --verbose link @react-native-community/netinfo",
    "getIP": "node getIP.js",
    "server": "cd ./server && npm run start",
    "serve": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server -d --config ./web/webpack.dev.config.js --inline --hot --colors",
    "bundle": "./node_modules/.bin/webpack -p --config ./web/webpack.config.js",
    "devBundle": "NODE_ENV=development ./node_modules/.bin/webpack --mode development -d --config ./web/webpack.config.js"
  },
  "dependencies": {
    "@react-native-community/netinfo": "3.2.1",
    "@types/styled-components": "^4.1.16",
    "@types/uuid": "^3.4.4",
    "react": "16.8.3",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "0.59.5",
    "react-native-elements": "^1.1.0",
    "react-native-vector-icons": "^6.5.0",
    "react-native-web": "^0.11.4",
    "react-relay": "^4.0.0",
    "react-relay-offline": "0.4.0",
    "react-scripts": "3.0.1",
    "relay": "^0.8.0-1",
    "relay-runtime": "^4.0.0",
    "styled-components": "4.3.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@svgr/webpack": "^4.3.0",
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.15",
    "@types/react-native": "^0.57.51",
    "@types/react-relay": "^1.3.14",
    "@types/react-test-renderer": "^16.8.1",
    "@types/relay-runtime": "^1.3.12",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-react-native-web": "^0.11.4",
    "babel-plugin-relay": "^4.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "graphql": "^14.2.1",
    "html-webpack-plugin": "^3.2.0",
    "ip": "1.1.5",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-native-svg-transformer": "^0.13.0",
    "react-test-renderer": "16.8.3",
    "relay-compiler": "^4.0.0",
    "relay-compiler-language-typescript": "^4.3.0",
    "relay-devtools": "^1.4.0",
    "ts-loader": "^6.0.3",
    "typescript": "^3.5.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.1",
    "yarn": "^1.15.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
